[run]
source = .
omit =
    */tests/*
    */test_*
    */__pycache__/*
    */node_modules/*
    */venv/*
    */.venv/*
    */dist/*
    */build/*
    */.next/*
    */coverage/*

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod
    @abc.abstractmethod

[html]
directory = coverage/html

[xml]
output = coverage/coverage.xml
