# Application Settings
STAGE=dev
DEFAULT_MODEL=gpt-3.5-turbo
MAX_TOKENS=1000

# Rate Limiting (optional)
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# Logging
LOG_LEVEL=info
